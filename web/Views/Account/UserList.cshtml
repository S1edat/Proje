@using web.Identity
@model IEnumerable<ApplicationUser>
@{
    ViewBag.Title = "UserList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<section class="content">
    <div class="row">
        <div class="col-xs-12">


            <div class="box">
                <div class="box-header">
                    <h3 class="box-title">Kullanıcı Listesi </h3>
                </div><!-- /.box-header -->


                <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">


                        <thead>
                            <tr>
                               
                                <th>Müşteri Kullanıcı Adı</th>
                                <th>Müşteri Adı</th>
                                <th>Müşteri Soyadı</th>
                                <th> Müşteri E-mail</th>

                               


                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)

                            {
                                <tr>
                                   
                                    <td>@item.UserName</td>
                                    <td>
                                        @item.Name
                                    </td>
                                    <td>
                                        @item.Surname
                                    </td>
                                    <td>
                                        @item.Email
                                    </td>

                                   

                                </tr>
                            }


                        </tbody>
                        <tfoot>
                            <tr>
                                <th>Müşteri Kullanıcı Adı</th>
                                <th>Müşteri Adı</th>
                                <th>Müşteri Soyadı</th>
                                <th> Müşteri E-mail</th>

                                <th></th>
                            </tr>
                        </tfoot>
                    </table>
                </div><!-- /.box-body -->
            </div><!-- /.box -->
        </div><!-- /.col -->
    </div><!-- /.row -->
</section><!-- /.content -->
